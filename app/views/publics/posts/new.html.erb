<%= render :partial => 'layouts/header' %>
<div class="container">
    <div class="row">
      	<div class="col-md-4 col-md-offset-5">

		<h1>投稿フォーム</h1><br>
		<%= form_for(@post,url:publics_user_posts_path) do |f| %>
		<h4>画像</h4>
		<%= f.file_field :post_image %><br>
		<h4>トレーニング内容</h4>
		<%= f.text_area :post_content %><br>
		<%= f.submit "投稿する", class: "btn btn-primary" %>
		<% end %>
		</div>
	</div>
</div>